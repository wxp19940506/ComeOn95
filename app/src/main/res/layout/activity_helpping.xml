<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_helpping"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.administrator.comeon95.HelppingActivity">
    <RelativeLayout
        android:id="@+id/rel1"
        android:layout_alignParentTop="true"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:gravity="center"
        android:background="@color/homeback_color"
        >
        <ImageView
            android:id="@+id/return1"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_marginTop="8dp"
            android:layout_marginLeft="15dp"
            android:src="@mipmap/return_icon"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:layout_marginRight="50dp"
            android:layout_toRightOf="@id/return1"
            android:gravity="center"
            android:layout_marginLeft="20dp"
            android:text="@string/helpping"
            android:textSize="16sp"/>
    </RelativeLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/rel1"
        android:background="@color/homeback_color"
        android:orientation="vertical">
        <ImageView
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:scaleType="fitXY"
            android:src="@mipmap/help1"/>
        <RelativeLayout
            android:id="@+id/lailai"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8dp"
            android:gravity="center_vertical">
            <ImageView
                android:id="@+id/my_speak"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:scaleType="fitXY"
                android:layout_marginTop="8dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="8dp"
                android:src="@mipmap/help2"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_toRightOf="@id/my_speak"
                android:layout_marginRight="8dp"
                android:gravity="center_vertical"
                android:layout_alignBaseline="@id/my_speak"
                android:layout_marginTop="8dp"
                android:textSize="@dimen/textSize"
                android:textColor="@color/textColor1"
                android:text="@string/lailai"/>
            <ImageView
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_alignParentRight="true"
                android:layout_marginRight="8dp"
                android:layout_marginTop="20dp"
                android:src="@mipmap/icon_turn"/>

        </RelativeLayout>
        <View
            android:id="@+id/view9"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_below="@id/lailai"
            android:layout_marginLeft="10dp"
            android:background="@color/background"/>
        <RelativeLayout
            android:id="@+id/myslf_rel2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8dp"
            android:gravity="center_vertical">
            <ImageView
                android:id="@+id/baibai"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:scaleType="fitXY"
                android:layout_marginTop="8dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="8dp"
                android:src="@mipmap/help3"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_toRightOf="@id/baibai"
                android:layout_marginRight="8dp"
                android:gravity="center_vertical"
                android:layout_alignBaseline="@id/baibai"
                android:layout_marginTop="8dp"
                android:textSize="@dimen/textSize"
                android:textColor="@color/textColor1"
                android:text="@string/baibai"/>
            <ImageView
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_alignParentRight="true"
                android:layout_marginRight="8dp"
                android:layout_marginTop="20dp"
                android:src="@mipmap/icon_turn"/>

        </RelativeLayout>
        <View
            android:id="@+id/view10"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_below="@id/myslf_rel2"
            android:layout_marginLeft="10dp"
            android:background="@color/background"/>
        <RelativeLayout
            android:id="@+id/telphone"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8dp"
            android:gravity="center_vertical">
            <ImageView
                android:id="@+id/phone"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:scaleType="fitXY"
                android:layout_marginTop="8dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="8dp"
                android:src="@mipmap/help4"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:layout_toRightOf="@id/phone"
                android:layout_marginRight="8dp"
                android:gravity="center_vertical"
                android:layout_alignBaseline="@id/phone"
                android:layout_marginTop="8dp"
                android:textSize="@dimen/textSize"
                android:textColor="@color/textColor1"
                android:text="@string/phone1"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:layout_alignParentRight="true"
                android:layout_marginRight="35dp"
                android:gravity="center_vertical"
                android:layout_alignBaseline="@id/phone"
                android:layout_marginTop="8dp"
                android:textSize="@dimen/textSize"
                android:textColor="@color/textColorhui"
                android:text="@string/phonenum"/>
            <ImageView
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_alignParentRight="true"
                android:layout_marginRight="8dp"
                android:layout_marginTop="20dp"
                android:src="@mipmap/icon_turn"/>

        </RelativeLayout>
        <View
            android:id="@+id/view2"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_below="@id/telphone"
            android:layout_marginLeft="10dp"
            android:background="@color/background"/>

    </LinearLayout>
</RelativeLayout>
